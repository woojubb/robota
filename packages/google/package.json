{
    "name": "@robota-sdk/google",
    "version": "1.0.3",
    "description": "Google AI integration for Robota SDK - Gemini Pro, Gemini Flash, function calling, and tool integration with Google's Generative AI",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/woojubb/robota.git",
        "directory": "packages/google"
    },
    "homepage": "https://robota.io/",
    "bugs": {
        "url": "https://github.com/woojubb/robota/issues"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit",
        "test": "vitest run --passWithNoTests",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix"
    },
    "keywords": [
        "ai",
        "agent",
        "llm",
        "google",
        "gemini",
        "gemini-pro",
        "gemini-flash",
        "palm",
        "google-ai",
        "function-calling",
        "typescript",
        "javascript",
        "chatbot",
        "conversational-ai",
        "artificial-intelligence",
        "machine-learning",
        "nlp",
        "natural-language-processing",
        "sdk",
        "library",
        "tool-integration",
        "robota",
        "google-api",
        "ai-integration",
        "generative-ai"
    ],
    "author": "Robota SDK Team",
    "license": "MIT",
    "dependencies": {
        "@google/generative-ai": "^0.20.0"
    },
    "peerDependencies": {
        "@robota-sdk/core": "workspace:*",
        "@robota-sdk/tools": "workspace:*"
    },
    "devDependencies": {
        "@robota-sdk/core": "workspace:*",
        "@robota-sdk/tools": "workspace:*",
        "@typescript-eslint/eslint-plugin": "^6.18.0",
        "@typescript-eslint/parser": "^6.18.0",
        "eslint": "^8.56.0",
        "rimraf": "^5.0.5",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "vitest": "^1.1.0"
    },
    "publishConfig": {
        "access": "public"
    }
}